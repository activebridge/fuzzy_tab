{
  "name": "Fuzzy Tab",
  "description" : "Fuzzy Tab allows you quickly navigate through tabs using fuzzy match.",
  "version": "1.0",
  "background": {
    "scripts": ["fuse.min.js", "background.js"]
  },
  "icons": { "16": "32px.png",
          "128": "128px.png" },
  "omnibox": { "keyword" : "f" },
  "browser_action": {
    "default_title": "Fuzzy Tab",
    "default_popup": "popup.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "chromeos": "Alt+O",
        "linux": "Alt+O",
        "mac": "Alt+O",
        "windows": "Alt+O"
      }
    },
    "close-tab": {
      "suggested_key": {
        "chromeos": "Alt+C",
        "linux": "Alt+C",
        "mac": "Alt+C",
        "windows": "Alt+C"
      },
      "description": "Close Focused Tab"
    }
  },
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "manifest_version": 2
}
